doctype html
html
  head
    meta(http-equiv='X-UA-Compatible' content='IE=edge,chrome=1')
    title Settings | GBL CENTUM
    meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport')
    link(rel='shortcut icon', type='image/x-icon', href='/images/icon.png')
    link(rel='stylesheet' href='/admin/assets/css/bootstrap.min.css')
    link(rel='stylesheet' href='https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i')
    link(rel='stylesheet' href='/admin/assets/css/ready.css')
    link(rel='stylesheet' href='/admin/assets/css/demo.css')
    // DATABLES
    link(rel="stylesheet", href="https://cdn.datatables.net/1.10.22/css/jquery.dataTables.min.css")
    link(rel="stylesheet", href="https://cdn.datatables.net/buttons/1.6.4/css/buttons.dataTables.min.css")

  body
    .wrapper
      .main-header
        .logo-header
          a.logo(href='#')
            img(src="/images/logo.png",width='70px', alt="")
          button.navbar-toggler.sidenav-toggler.ml-auto(type='button' data-toggle='collapse' data-target='collapse' aria-controls='sidebar' aria-expanded='false' aria-label='Toggle navigation')
            span.navbar-toggler-icon
          button.topbar-toggler.more
            i.la.la-ellipsis-v
        nav.navbar.navbar-header.navbar-expand-lg
          .container-fluid
            
            ul.navbar-nav.topbar-nav.ml-md-auto.align-items-center
              
              
              li.nav-item.dropdown
                a.dropdown-toggle.profile-pic(data-toggle='dropdown' href='#' aria-expanded='false')
                  img.img-circle(src='/images/user.png' alt='user-img' width='36')
                  span #{user.first_name} #{user.last_name}
                ul.dropdown-menu.dropdown-user
                  
                
                  a.dropdown-item(href='/administrator/log_out')
                    i.fa.fa-power-off
                    |  Logout
                // /.dropdown-user
      .sidebar
        .scrollbar-inner.sidebar-wrapper
          .user
            .photo
              img(src='/images/user.png')
            .info
              a(data-toggle='collapse' href='#collapseExample' aria-expanded='true')
                span
                  | #{user.first_name} #{user.last_name}
                  span.user-level Administrator
                
              .clearfix
             
                 
          ul.nav
            li.nav-item
              a(href='/administrator/companies')
                i.la.la-institution
                p Companies
                span.badge.badge-primary #{companies.length}
            li.nav-item
              a(href='/administrator/licenses')
                i.la.la-unlock
                p Licenses
               
        
           
               
            li.nav-item.active
              a(href='/administrator/settings')
                i.la.la-gear
                p Settings
             
                
            
      .main-panel
        .content
          .container-fluid
            h4.page-title Settings
            .row
             .col-md-12 
              != messages('message', locals)
             .col-md-12
                ul#myTab.nav.nav-tabs(role='tablist')
                  li.nav-item
                    a#bank-tab.nav-link.active(data-toggle='tab' href='#bank' role='tab' aria-controls='bank' aria-selected='true') Banks
                  li.nav-item
                    a#currency-tab.nav-link(data-toggle='tab' href='#currency' role='tab' aria-controls='currency' aria-selected='false') Currencies
                #myTabContent.tab-content
                  // bank
                  #bank.tab-pane.fade.show.active(role='tabpanel' aria-labelledby='bank-tab') 
                    .row(style='margin-top:20px')
                     .col-md-12
                       button.btn.btn-primary(data-toggle="modal" data-target="#addBank") 
                        i.la.la-plus
                        &nbsp;
                        | Add Bank
                    .row(style='margin-top:20px')
                      .col-md-12
                        table.table.mytable
                              thead
                               tr
                                th Title
                                th Actions
                              tbody
                               each bank in banks
                                tr
                                 td #{bank.title} bank
                                 td
                                    a(href="/administrator/edit_bank_details/#{bank.id}" data-toggle="modal" data-target="#editBankDetails" data-whatever=bank.id)
                                      i.la.la-edit
                                    a(href="/administrator/delete_bank/#{bank.id}") 
                                     i.la.la-trash(style='color:red')



                  // currency
                  #currency.tab-pane.fade.show(role='tabpanel' aria-labelledby='currency-tab') 
                    .row(style='margin-top:20px')
                      .col-md-12
                       button.btn.btn-primary(data-toggle="modal" data-target="#addCurrency")
                        i.la.la-plus
                        &nbsp;
                        | Add Currency
                    .row(style='margin-top:20px')
                      .col-md-12
                        table.table.mytable
                              thead
                               tr
                                th Country
                                th Code
                                th Actions
                              tbody
                               each currency in currencys
                                tr
                                 td=currency.country
                                 td=currency.code
                                 td
                                    a(href="/administrator/edit_currency_details/#{currency.id}" data-toggle="modal" data-target="#editCurrencyDetails" data-whatever=currency.id)
                                      i.la.la-edit
                                    a(href="/administrator/delete_currency/#{currency.id}") 
                                     i.la.la-trash(style='color:red')

                                  
                                
                               
                 
                 
                   
              
              
    // Banks
    #addBank.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
     .modal-dialog(role='document')
      .modal-content
        .modal-header
            h5#exampleModalLabel.modal-title Add New Bank
            button.close(type='button' data-dismiss='modal' aria-label='Close')
            span(aria-hidden='true') ×
        .modal-body
            form.form(action='/administrator/add_new_bank',method='POST')
              .form-group
               label(for="title") Title
               input.form-control(type="text", name="title",required,placeholder='Title')
             
                
            
              .form-group
               button.btn.btn-primary Save
        .modal-footer
            button.btn.btn-secondary(type='button' data-dismiss='modal') Close
    #editBankDetails.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
     .modal-dialog(role='document')
        .modal-content
         .modal-header
            h5#exampleModalLabel.modal-title Edit Bank
            button.close(type='button' data-dismiss='modal' aria-label='Close')
             span(aria-hidden='true') ×
         .modal-body
           
            form(action='/administrator/edit_bank_details' method='POST')
             
             input#bank_id.form-control(type='hidden' name='id')
             .form-group
               label(for="title") Title
               input.form-control#bank_title(type="text", name="title",required,placeholder='title')
             
             .form-group
               button.btn.btn-primary Save
               
             
         .modal-footer
            button.btn.btn-secondary(type='button' data-dismiss='modal') Close       
            
