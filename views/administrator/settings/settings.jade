       $(document).ready( function () {
             $('.mytable').DataTable({
                dom: 'Bfrtip',
                buttons: [
                    'copy', 'csv', 'excel', 'pdf', 'print'
                ]
            } );

             // Bank Details
            $('#editBankDetails').on('show.bs.modal', function (event) {
                var button = $(event.relatedTarget) // Button that triggered the modal
                var recipient = button.data('whatever') // Extract info from data-* attributes
               
                var bank = {}
                $.get('/administrator/get_bank_details/'+recipient, function( data ) {
                 
                   bank=data.bank
                })
                setTimeout(()=>{
                 var modal = $(this)
                   modal.find('.modal-body #bank_id').val(recipient)
                   modal.find('.modal-body #bank_title').val(bank.title)
                 
                 
                },1000) 
            })

            // Currencies
            $('#editCurrencyDetails').on('show.bs.modal', function (event) {
                var button = $(event.relatedTarget) // Button that triggered the modal
                var recipient = button.data('whatever') // Extract info from data-* attributes
               
                var currency = {}
                $.get('/administrator/get_currency_details/'+recipient, function( data ) {
                 
                   currency=data.currency
                })
                setTimeout(()=>{
                 var modal = $(this)
                   modal.find('.modal-body #currency_id').val(recipient)
                   modal.find('.modal-body #currency_code').val(currency.code)
                 
                 
                },1000) 
            })

            
       })
   
